#include <limits.h>
#include <math.h>
#include <stdbool.h>
 


#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    int t;
    float p=0,n=0,z=0;
    scanf("%d",&t);
 

 

    int arr[t];
    for(int i = 0; i < t;i++){
       scanf("%d",&arr[i]);
        if(arr[i]>0)
            p++;
           else if(arr[i]<0)
               n++;
               else
               z++;
    }
    printf("%.5f\n%.5f\n%.5f",(float)p/t,(float)n/t,(float)z/t);
    return 0;
}
